<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">

       <record id="customer_number" model="ir.sequence">
           <field name="name">Customer Number</field>
           <field name="code">customer_res.partner.sequence</field>
           <field name="prefix">PK</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

       <record id="vendor_number" model="ir.sequence">
           <field name="name">Vendor Number</field>
           <field name="code">vendor_res.partner.sequence</field>
           <field name="prefix">DP</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

        <record id="employee_code" model="ir.sequence">
           <field name="name">Employee Code</field>
           <field name="code">employee_code</field>
           <field name="prefix">EC</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

    </data>
</odoo>
